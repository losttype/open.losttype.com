h1 Open Source at Lost&nbsp;Type

ul.ghrepo-list
  each project in public._data
    //- Hide forks for now
    if !project.origin
      li.ghrepo-item
        github-repo(src="#{ project.repo }")
        noscript
          a.ghrepo(data-ghrepo="url" href="https://github.com/#{ project. repo }")
            .ghrepo-title(data-ghrepo="fullName") #{ project.repo }
            .ghrepo-description
              div(data-ghrepo="description") #{ project.desc }
